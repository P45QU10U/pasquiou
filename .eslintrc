{
  "extends": ["wesbos"],
  "rules": {
    "no-console": 2,
    "react/prop-types": 0,
    "no-nested-ternary": 0,
    "prettier/prettier": [
      "error",
      {
        "semi": false,
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}
